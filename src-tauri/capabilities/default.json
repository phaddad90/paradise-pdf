{
  "identifier": "main-capability",
  "description": "Main window capabilities",
  "windows": ["main"],
  "remote": {
    "urls": ["http://localhost:1420", "http://localhost:1420/*"]
  },
  "permissions": [
    "core:default",
    "dialog:default",
    "dialog:allow-open",
    "dialog:allow-message",
    "dialog:allow-ask",
    "dialog:allow-confirm"
  ]
}
